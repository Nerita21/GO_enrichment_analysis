{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Run GO clustering",
        "type": "python",
        "request": "launch",
        "program": "${workspaceFolder}/clustering_goterms_3.py",
        "console": "integratedTerminal",
        "cwd": "${workspaceFolder}",
        "args": [
          "--input-file", "${workspaceFolder}/${input.tsv}",
          "--obo", "go-basic.obo",
          "--gaf", "goa_human.gaf.gz",
          "--taxon", "9606",
          "--exclude-evidence", "IEA,ND",
          "--sim-threshold", "0.3",
          "--out-prefix", "goterm",
          "--no-show"
        ]
      }
    ]
  }
